#! /usr/bin/bash

ABS_PATH=$(readlink -f "$0")
SERVERLESS_PATH=$(dirname $ABS_PATH)
BANNER_FILE_PATH="$SERVERLESS_PATH/files/banner.jpeg"
DATA_FILE_PATH="$SERVERLESS_PATH/files/lore.jpeg"

aws s3 cp "$BANNER_FILE_PATH" "s3://sen-cruddur-uploaded-avatars/avatars/banner.jpeg"
aws s3 cp "$DATA_FILE_PATH" "s3://sen-cruddur-uploaded-avatars/avatars/lore.jpeg"